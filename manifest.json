{
    "manifest_version":2,
    "name":"BNTV",
    "version":"3.0.0",
    "description":"Enhances Nimo with new features,emotes and more.",
    "icons":{
        "16": "icons/BNTV16.png",
        "48": "icons/BNTV48.png",
        "128": "icons/BNTV128.png"
    },
    "content_scripts":[
        {
            "matches":[
                "*://*.nimo.tv/*"
            ],
            "js":
            [
                "lib/jquery3.5.1.min.js"
                ,"lib/bootstrap3.4.1.js"
                ,"scripts/Mention_AutoComplete_TextArea.js"
                ,"scripts/Mention_Emote.js"
                ,"scripts/Other_Scripts.js"
                ,"scripts/Script_BNTV.js"
            ],
            "css":
            [
                "styles/BNTV_Style_tooltip.css"
                ,"styles/BNTV_Style.css"
                ,"styles/BNTV_Style_CB.css"
            ],
            "run_at": "document_end"
        }
    ]
}